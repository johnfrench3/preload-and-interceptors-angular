{
  "name": "angular-preload",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-ng": "ng serve --proxy-config proxy.conf.json --port 7626 --open",
    "start": "WWW=dist/angular-preload node server.js",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "map": "source-map-explorer dist/angular-preload/*es2015*.js*",
    "full-stack": "concurrently \"npm run backend\" \"npm run start-ng\"",
    "backend": "json-server-auth --watch db.json --routes routes.json --port 7627 --delay 1000 --middlewares ./node_modules/json-server-reset"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.7",
    "@angular/common": "^11.2.7",
    "@angular/compiler": "^11.2.7",
    "@angular/core": "^11.2.7",
    "@angular/forms": "^11.2.7",
    "@angular/platform-browser": "^11.2.7",
    "@angular/platform-browser-dynamic": "^11.2.7",
    "@angular/router": "^11.2.7",
    "@ngrx/data": "^11.1.0",
    "@ngrx/effects": "^11.1.0",
    "@ngrx/entity": "^11.1.0",
    "@ngrx/store": "^11.1.0",
    "@ngrx/store-devtools": "^11.1.0",
    "bulma": "^0.9.1",
    "core-js": "^3.7.0",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.6",
    "@angular/cli": "^11.2.6",
    "@angular/compiler-cli": "^11.2.7",
    "@angular/language-service": "^11.2.7",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.8",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-server": "^0.16.3",
    "json-server-auth": "^2.0.2",
    "json-server-reset": "^1.3.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  }
}
